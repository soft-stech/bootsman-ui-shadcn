{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "dist",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "./types.d.ts"
    ]
  },
  "include": ["env.d.ts", "src/**/*", "types.d.ts"],
  "exclude": ["src/**/__tests__/*", "src/stories/**/*"]
}
